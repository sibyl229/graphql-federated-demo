input {

  s3 {
    bucket => "mod-datadumps"
    prefix => "3.0.0/VEP/WB"
    exclude_pattern => "VEP_WB_(?!2)\.txt"
    sincedb_path => "/dev/null"
  }
}

filter {

  if [message] =~ /^#/ {
    drop {}
  }

  csv {
    separator => "	"  # An actual tab chararcter. Please don't change.
    columns => [
      "Uploaded_variation",
      "Location",
      "Allele",
      "Gene",
      "Feature",
      "Feature_type",
      "Consequence",
      "cDNA_position",
      "CDS_position",
      "Protein_position",
      "Amino_acids",
      "Codons",
      "Existing_variation",
      "Extra"
    ]
  }

  kv {
    source => "Extra"
    field_split => ";"
  }
}

output {
  stdout {
  }
}
